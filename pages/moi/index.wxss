/* pages/moi/index.wxss */
.container{
  background: whitesmoke;
  width: 100%;
  height: 100%;
  margin: 0%;
  padding: 0%;
}
 .usercard{ 
   /* background:url('data:image/png;base64,'); */
  background: #E6007D;
  margin: 3vw;
  border-top-left-radius: 2vw;
  border-top-right-radius: 2vw;
  color: whitesmoke;
  box-shadow: 3px 0px 10px rgba(0, 0, 0, 0.2);  
  width: 94vw;
}
.userinfo{
  display: flex;
  padding: 4vw;
  font-size: 4vw;
}
.nameinfo{
  margin-top: 6vw;
  width: 51.6vw;
  text-align: left;
  align-items: center;
}

.userimage{
  width: 20vw;
  height: 20vw;
  border-radius: 50%;
  overflow:hidden;  
  display: block;  
  margin: 2.9vw;  
  border: 2px solid #fff;  
  box-shadow: 3px 3px 10px rgba(0, 0, 0, 0.2);  
}
.usercontainer{
  display: block;
}
.subview{
  background: rgba(255, 255, 255, 0.2);
  display: flex;
  color: whitesmoke;
  justify-content: space-between;
  font-size: 4vw;
  padding:4pt;
}
.subview-item:hover{
  font-size: 5vw;
}

.subview-item text{
  display: block;
  text-align: center;
}
.userbutton image{
  width:8vw;
  height:8vw;
}
.username{
  font-size: 4vw;
  display: block;
}

.myorder{
  padding: 8pt;
  margin-top: 8pt;
  margin-bottom: 8pt;
  background:white;
  width: 92vw;
  font-size: 4vw;
}
.orderlist{
  display: flex;
  justify-content: space-between;
  color: #b3b2b2;
  margin-bottom: 4pt;
  margin-right: 4pt;
  margin-left:4pt;
}

.orderlist-item{
  display: flex;
  align-items: center;
  justify-content: center;
}
.orderlist image{
  width: 6vw;
  height: 6vw;
}

.myorderinfo{
  border-top: 1px solid whitesmoke;
  padding-top: 8pt;
  display: flex;
  justify-content: space-around;
  text-align: center;
  font-size: 4vw;
}
.myorderinfo image{
  width: 8vw;
  height: 8vw;
}
.myorderinfo-item text{
  display: block;
}

.ads{
  display: block;
  background:white;
  width: 100%;
}

.ads-list{
  display: flex;
  justify-content: space-around;
}

.ads image{
  width: 8vw;
  height: 8vw;
}

.ads-item{
  display: flex;
  color:black;
  align-items: center;
  justify-content: center;
  margin-top: 12pt;
  margin-bottom: 12pt;
  font-size: 4vw;
}
